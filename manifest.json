{
    "manifest_version": 3,
    "name": "Boldly Read",
    "version": "2.0",
    "action": {
      "default_icon": "icon.png", 
      "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
          "matches":["<all_urls>"],
          "js": [
            "./main.js", 
            "popup/popup.js"
          ]
        }
      ]
  }